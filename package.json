{
    "name": "sna-fileviewer",
    "displayName": "SNA-FileViewer",
    "description": "A viewer for ZX Spectrum snapshot (.sna) files.",
    "version": "1.0.1",
    "publisher": "maziac",
    "author": {
        "name": "Thomas Busse"
    },
    "license": "MIT",
    "keywords": [
        "assembler",
        "binary",
        "snapshot",
        "z80",
        "zx",
        "spectrum"
    ],
    "repository": {
        "type": "git",
        "url": "https://github.com/maziac/sna-fileviewer"
    },
    "engines": {
        "vscode": "^1.52.0"
    },
    "icon": "assets/icon.png",
    "categories": [
        "Programming Languages"
    ],
    "activationEvents": [
        "onCustomEditor:sna-fileviewer.viewer",
        "onCommand:sna-fileviewer.openSna"
    ],
    "main": "./out/extension",
    "contributes": {
        "customEditors": [
            {
                "viewType": "sna-fileviewer.viewer",
                "displayName": "ZX SNA File Viewer",
                "selector": [
                    {
                        "filenamePattern": "*.sna"
                    }
                ],
                "priority": "default"
            }
        ]
    },
    "scripts": {
        "test-compile": "tsc -p ./",
        "compile": "tsc -p ./",
        "watch": "tsc -watch -p ./",
        "test": "nyc --reporter=text mocha --ui tdd -r ts-node/register test/**/*.ts"
    },
    "devDependencies": {
        "@types/glob": "^7.1.3",
        "@types/mocha": "^8.0.4",
        "@types/node": "^12.11.7",
        "@types/vscode": "^1.52.0",
        "@typescript-eslint/eslint-plugin": "^4.9.0",
        "@typescript-eslint/parser": "^4.9.0",
        "eslint": "^7.15.0",
        "glob": "^7.1.6",
        "mocha": "^8.1.3",
        "typescript": "^4.1.2",
        "vscode-test": "^1.4.1"
    },
    "dependencies": {
        "gif-writer": "^0.9.4",
        "path": "^0.12.7"
    }
}
